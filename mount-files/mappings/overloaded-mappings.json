{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "url": "/symlinked"
      },
      "response": {
        "status": 200,
        "bodyFileName": "dynamic/whatever.json",
        "headers": {
          "Content-Type": "application/json; charset=utf-8",
          "X-Overloaded": "overloaded"
        }
      }
    },    {
      "request": {
        "method": "GET",
        "url": "/static"
      },
      "response": {
        "status": 200,
        "bodyFileName": "static/sample/template.hbs",
        "headers": {
          "Content-Type": "application/json; charset=utf-8",
          "X-Overloaded": "overloaded"
        },
        "transformers" : [ "response-template" ]
      }
    }

  ]
}